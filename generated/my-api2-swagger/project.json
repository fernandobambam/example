{
  "name": "my-api2-swagger",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "targets": {
    "build": {
      "executor": "nx:noop",
      "outputs": ["generated/my-api2-swagger"]
    },
    "codegen": {
      "executor": "@nx-dotnet/core:openapi-codegen",
      "options": {
        "openapiJsonPath": "generated/my-api2-swagger/swagger.json",
        "outputProject": "generated-my-api2-types"
      },
      "dependsOn": ["^swagger"]
    }
  },
  "implicitDependencies": ["my-api2"]
}
